{
  "author": "Wilfried Barth <wilfried.barth.prof@gmail.com>",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/free-brands-svg-icons": "5.15.1",
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@fortawesome/react-fontawesome": "0.1.12",
    "disqus-react": "1.0.10",
    "gatsby": "2.23.20",
    "gatsby-plugin-catch-links": "2.3.10",
    "gatsby-plugin-google-analytics": "2.3.10",
    "gatsby-plugin-netlify": "2.3.10",
    "gatsby-plugin-react-helmet": "3.3.9",
    "gatsby-plugin-remove-serviceworker": "1.0.0",
    "gatsby-plugin-sharp": "2.6.17",
    "gatsby-plugin-sitemap": "2.4.10",
    "gatsby-plugin-typography": "2.5.9",
    "gatsby-remark-images": "3.3.17",
    "gatsby-remark-prismjs": "3.5.9",
    "gatsby-source-filesystem": "2.3.18",
    "gatsby-transformer-remark": "2.8.23",
    "prismjs": "1.21.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-helmet": "5.2.1",
    "react-typography": "0.16.19",
    "typography": "0.16.19",
    "typography-theme-lincoln": "0.16.19"
  },
  "description": "Wilfried Barth's Blog",
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.3",
    "prettier": "2.0.5",
    "stylelint": "13.6.1",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "parser": "babel-eslint",
    "rules": {
      "react/jsx-sort-props": "error"
    }
  },
  "license": "MIT",
  "name": "wb-blog",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "plugins": [
      "stylelint-order"
    ],
    "rules": {
      "order/order": [
        "custom-properties",
        "declarations"
      ],
      "order/properties-alphabetical-order": true
    }
  },
  "version": "1.0.0"
}
