{
  "author": "Wilfried Barth <wilfried.barth.prof@gmail.com>",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.4.0",
    "@fortawesome/free-brands-svg-icons": "6.4.0",
    "@fortawesome/free-solid-svg-icons": "6.4.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "gatsby": "5.11.0",
    "gatsby-plugin-catch-links": "5.11.0",
    "gatsby-plugin-google-gtag": "^5.11.0",
    "gatsby-plugin-netlify": "5.1.0",
    "gatsby-plugin-react-helmet": "6.11.0",
    "gatsby-plugin-remove-serviceworker": "1.0.0",
    "gatsby-plugin-sharp": "5.11.0",
    "gatsby-plugin-sitemap": "6.11.0",
    "gatsby-plugin-typography": "5.11.0",
    "gatsby-remark-images": "7.11.0",
    "gatsby-remark-prismjs": "7.11.0",
    "gatsby-source-filesystem": "5.11.0",
    "gatsby-transformer-remark": "6.11.0",
    "prismjs": "1.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-typography": "0.16.23",
    "typography": "0.16.21",
    "typography-theme-lincoln": "0.16.19"
  },
  "description": "Wilfried Barth's Blog",
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "prettier": "3.0.0",
    "stylelint": "15.10.1",
    "stylelint-config-standard": "34.0.0",
    "stylelint-order": "6.0.3"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "parser": "babel-eslint",
    "rules": {
      "react/jsx-sort-props": "error"
    }
  },
  "license": "MIT",
  "name": "wb-blog",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "plugins": [
      "stylelint-order"
    ],
    "rules": {
      "order/order": [
        "custom-properties",
        "declarations"
      ],
      "order/properties-alphabetical-order": true
    }
  },
  "version": "1.0.0"
}
