{
  "author": "Wilfried Barth <wilfried.barth.prof@gmail.com>",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.3.0",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.17",
    "gatsby": "4.9.3",
    "gatsby-plugin-catch-links": "4.9.0",
    "gatsby-plugin-google-analytics": "4.9.0",
    "gatsby-plugin-netlify": "2.11.1",
    "gatsby-plugin-react-helmet": "5.9.0",
    "gatsby-plugin-remove-serviceworker": "1.0.0",
    "gatsby-plugin-sharp": "4.9.1",
    "gatsby-plugin-sitemap": "5.9.0",
    "gatsby-plugin-typography": "4.9.0",
    "gatsby-remark-images": "6.9.1",
    "gatsby-remark-prismjs": "6.9.0",
    "gatsby-source-filesystem": "4.9.1",
    "gatsby-transformer-remark": "5.9.1",
    "prismjs": "1.27.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-typography": "0.16.20",
    "typography": "0.16.21",
    "typography-theme-lincoln": "0.16.19"
  },
  "description": "Wilfried Barth's Blog",
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.29.4",
    "prettier": "2.5.1",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "stylelint-order": "4.1.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "parser": "babel-eslint",
    "rules": {
      "react/jsx-sort-props": "error"
    }
  },
  "license": "MIT",
  "name": "wb-blog",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "plugins": [
      "stylelint-order"
    ],
    "rules": {
      "order/order": [
        "custom-properties",
        "declarations"
      ],
      "order/properties-alphabetical-order": true
    }
  },
  "version": "1.0.0"
}
