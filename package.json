{
  "author": "Wilfried Barth <wilfried.barth.prof@gmail.com>",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/free-brands-svg-icons": "5.15.1",
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@fortawesome/react-fontawesome": "0.1.13",
    "disqus-react": "1.0.10",
    "gatsby": "2.28.0",
    "gatsby-plugin-catch-links": "2.6.0",
    "gatsby-plugin-google-analytics": "2.7.0",
    "gatsby-plugin-netlify": "2.7.0",
    "gatsby-plugin-react-helmet": "3.6.0",
    "gatsby-plugin-remove-serviceworker": "1.0.0",
    "gatsby-plugin-sharp": "2.10.0",
    "gatsby-plugin-sitemap": "2.8.0",
    "gatsby-plugin-typography": "2.8.0",
    "gatsby-remark-images": "3.7.0",
    "gatsby-remark-prismjs": "3.9.0",
    "gatsby-source-filesystem": "2.7.0",
    "gatsby-transformer-remark": "2.12.0",
    "prismjs": "1.22.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-typography": "0.16.19",
    "typography": "0.16.19",
    "typography-theme-lincoln": "0.16.19"
  },
  "description": "Wilfried Barth's Blog",
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "7.15.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.2.0",
    "eslint-plugin-react": "7.21.5",
    "prettier": "2.2.1",
    "stylelint": "13.8.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "parser": "babel-eslint",
    "rules": {
      "react/jsx-sort-props": "error"
    }
  },
  "license": "MIT",
  "name": "wb-blog",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "plugins": [
      "stylelint-order"
    ],
    "rules": {
      "order/order": [
        "custom-properties",
        "declarations"
      ],
      "order/properties-alphabetical-order": true
    }
  },
  "version": "1.0.0"
}
