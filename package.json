{
  "author": "Wilfried Barth <wilfried.barth.prof@gmail.com>",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "disqus-react": "^1.0.7",
    "gatsby": "^2.20.2",
    "gatsby-plugin-catch-links": "^2.2.0",
    "gatsby-plugin-google-analytics": "^2.2.0",
    "gatsby-plugin-netlify": "^2.2.0",
    "gatsby-plugin-react-helmet": "^3.2.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^2.5.1",
    "gatsby-plugin-sitemap": "^2.3.0",
    "gatsby-plugin-typography": "^2.4.0",
    "gatsby-remark-images": "^3.2.0",
    "gatsby-remark-prismjs": "^3.4.0",
    "gatsby-source-filesystem": "^2.2.1",
    "gatsby-transformer-remark": "^2.7.0",
    "prismjs": "^1.19.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-lincoln": "^0.16.19"
  },
  "description": "Wilfried Barth's Blog",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "prettier": "^2.0.1",
    "stylelint": "^13.2.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "parser": "babel-eslint",
    "rules": {
      "react/jsx-sort-props": "error"
    }
  },
  "license": "MIT",
  "name": "wb-blog",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "plugins": [
      "stylelint-order"
    ],
    "rules": {
      "order/order": [
        "custom-properties",
        "declarations"
      ],
      "order/properties-alphabetical-order": true
    }
  },
  "version": "1.0.0"
}
